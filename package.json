    "compile": "browserify src/main.jsx -t babelify -o dist/index.js",
    "watch": "onchange 'src/**/*.jsx' -- npm run build",
    "start": "http-server ."
  },
  "author": "Rohan Allison",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.15.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "http-server": "^0.9.0",
    "onchange": "^3.0.2"
  }
}
